import datetime
d1 = raw_input('input a start date(yyyy/mm/dd):').split('/')
d2 = raw_input('input a end date(yyyy/mm/dd):').split('/')

date1 = datetime.datetime(int(d1[0]), int(d1[1]), int(d1[2]))#datetime参数为整型
date2 = datetime.datetime(int(d2[0]), int(d2[1]), int(d2[2]))
print (date2 - date1).days #直接打印，简单粗暴
