import datetime
d1 = raw_input('input a start date(yyyy/mm/dd):').split('/')
d2 = raw_input('input a end date(yyyy/mm/dd):').split('/')
y1 = int(d1[0])#datetime只接受整型
m1 = int(d1[1])
dd1 = int(d1[2])
y2 = int(d2[0])
m2 = int(d2[1])
dd2 = int(d2[2])

date1 = datetime.datetime(y1, m1, dd1)
date2 = datetime.datetime(y2, m2, dd2)
print (date2 - date1).days #直接打印，简单粗暴
